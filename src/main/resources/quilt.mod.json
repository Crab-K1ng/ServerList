{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "adds a server list",
      "contributors": {
        "CrabKing": "Developer"
      },

      "contact": {
        "homepage": "https://discord.com/channels/1198501071363002408/1302806149367861288",
        "issues": "https://github.com/Crab-K1ng/ServerList/issues",
        "sources": "https://github.com/Crab-K1ng/ServerList"
      },

      "icon": "assets/server_list/icon.png"
    },

    "entrypoints": {
      "init": "io.github.CrabK1ng.ServerList.ServerList"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": ">=2.3.1"
      },
      {
        "id": "cosmicreach",
        "versions": ">=0.3.4"
      }
    ]
  },

  "access_widener": "server_list.accesswidener",
  "mixin": "server_list.mixins.json"
}
